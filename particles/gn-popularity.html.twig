{% block particle %}
<div class='popularity line-data'>

{% if particle.title %}<h4 class='g-title'>{{ particle.title|raw }}</h4>{% endif %}
<div class='daily'>Daily<span class="stat">{{ popularity('daily') }}</span></div>
<div class='monthly'>Monthly<span class="stat">{{ popularity('monthly') }}</span></div>
<div class='total'>Totals<span class="stat">{{ popularity('total') }}</span></div>
</div>
{% endblock %}