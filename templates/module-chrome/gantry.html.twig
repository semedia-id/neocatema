{% set title %}
    {% block title %}{% endblock %}
{%- endset -%}

{% set module %}
    {% block module %}{% endblock %}
{%- endset -%}

{% if module|trim %}
<div class="moduletable{{ chrome.class ? ' ' ~ chrome.class }}">
    {% if title|trim %}
        <h4 class="g-title">{{ title }}</h4>
    {% endif %}

    {{ module }}
</div>
{% endif %}